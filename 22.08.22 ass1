#include<stdio.h>
void main()
{
	int a[100][100],n;
	int i,j,r,c,sum;
	printf("how many rows\n");
	scanf("%d",&r);
	printf("how many cols\n");
	scanf("%d",&c);
	printf("enter row number:");
	scanf("%d",&n);
	printf("enter elements into array\n");
	for(i=1;i<=r;i++)
	{
		for(j=1;j<=c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=n;i<=n;i++)
	{
		sum=0;
		for(j=1;j<=c;j++)
		{
			sum=sum+a[i][j];
		}
		printf("%d row sum=%d\n",i,sum);
	}
	
}
