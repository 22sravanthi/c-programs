sno=int(input('studentno:'))
sname=input('student name:')
group=input('group:')
gender=input('Gender:')
phno=int(input('phone number:'))
add=input('city:')
dgper=float(input('percentage:'))
print('STUDENT DETAILS')
print("Studentno:",sno)
print("Student name:",sname)
print('Group:',group)
print('Gender:',gender)
print('Phone no:',phno)
print('Address:',add)
print('Degree percentage:',dgper)
